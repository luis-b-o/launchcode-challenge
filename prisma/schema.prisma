generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quote {
  id              String     @id @default(cuid())
  origin          String
  destination     String
  departDate      DateTime
  returnDate      DateTime
  adults          Int
  transportation  String?
  name            String
  email           String
}